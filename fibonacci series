#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
    int a=0,b=1,n,i,f;
    scanf("%d",&n);
    printf("%d %d ",a,b);
    for(i=1;i<=n;i++)
    {
        f= a+b;
        printf("%d ",f);
        a=b;
        b=f;
    }
    return 0;
}
